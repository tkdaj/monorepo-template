{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "bracketSameLine": false,
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "useTabs": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.html",
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
